{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "DatabaseSettings": {
    "StandardSecurity": "Server=myServerAddress;Database=myDataBase;User Id=myUsername;Password = myPassword",
    "TrustedConnection": "Server=myServerAddress;Database=myDataBase;Trusted_Connection=True",
    "SQLServerInstance": "Server=myServerName\\myInstanceName;Database=myDataBase;User Id=myUsername;Password=myPassword",
    "NonStandardPort": "Server=myServerName,myPortNumber;Database=myDataBase;User Id=myUsername;Password=myPassword"
  }
}